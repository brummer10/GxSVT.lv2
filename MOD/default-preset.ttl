@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://guitarix.sourceforge.net/plugins/gx_ampegsvt_#_ampegsvt_> ;
	lv2:port [
		lv2:symbol "BASS" ;
		pset:value 0.5
	] , [
		lv2:symbol "BYPASS" ;
		pset:value 1.0
	] , [
		lv2:symbol "CABSWITCH" ;
		pset:value 0.0
	] , [
		lv2:symbol "HIGHSWITCH" ;
		pset:value 0.0
	] , [
		lv2:symbol "LOWSWITCH" ;
		pset:value 1.0
	] , [
		lv2:symbol "MIDDLE" ;
		pset:value 0.5
	] , [
		lv2:symbol "MIDSWITCH" ;
		pset:value 1.0
	] , [
		lv2:symbol "TREBLE" ;
		pset:value 0.5
	] , [
		lv2:symbol "VOLUME" ;
		pset:value 0.2
	] .

